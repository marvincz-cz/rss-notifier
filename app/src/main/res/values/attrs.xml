<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />
    <attr name="icon" format="reference" />
    <attr name="actionIcon" format="reference" />

    <declare-styleable name="OneLineItem">
        <attr name="title" />
        <attr name="icon" />
        <attr name="actionIcon" />
    </declare-styleable>

    <declare-styleable name="TwoLineItem">
        <attr name="title" />
        <attr name="description" format="string" />
        <attr name="icon" />
        <attr name="actionIcon" />
    </declare-styleable>
</resources>